#!/usr/bin/env bash
KUBASH_VERSION='v1.14.1'
: ${KUBASH_DIR:=$HOME/.kubash}
# import our includes
. $KUBASH_DIR/lib/kinc.bash
TMP=$(mktemp -d --suffix='.kubash.tmp' 2>/dev/null || mktemp -d -t '.kubash.tmp')
trap killtmp EXIT
# exit on errors
set -e

main () {
  checks
  # save original io
  exec 3>&1 4>&2
  # Let's display everything on stderr.
  exec 1>&2

  parse_opts "$@"

  exit 0
  # End main block
}

main "$@"
